ot;lazyImages&quot;:true,&quot;normalize&quot;:true,&quot;safeMode&quot;:false,&quot;settings&quot;:{&quot;position&quot;:false},&quot;theme&quot;:&quot;light&quot;},&quot;terms&quot;:[{&quot;_id&quot;:&quot;5f33ff472cae25001872046f&quot;,&quot;term&quot;:&quot;Vendor&quot;,&quot;definition&quot;:&quot;The VTEX store who owns the product that will  be sold.&quot;},{&quot;_id&quot;:&quot;5f34006a12dde106711536a9&quot;,&quot;term&quot;:&quot;vendor&quot;,&quot;definition&quot;:&quot;The VTEX store who owns the product that will be sold.&quot;},{&quot;_id&quot;:&quot;5f4ea348c13cb000284cf763&quot;,&quot;term&quot;:&quot;Broadcaster&quot;,&quot;definition&quot;:&quot;The VTEX agent that notifies external systems (like the connectors) about any changes in the VTEX Catalog.&quot;},{&quot;_id&quot;:&quot;5f4ea3cc5a2914004c38a504&quot;,&quot;term&quot;:&quot;Offer&quot;,&quot;definition&quot;:&quot;Vendor's SKU associated to a channel, with specific conditions of price, inventory and delivery.&quot;},{&quot;_id&quot;:&quot;5f4ea3d213883700110480cd&quot;,&quot;term&quot;:&quot;offer&quot;,&quot;definition&quot;:&quot;Vendor's SKU associated to a channel, with specific conditions of price, inventory and delivery.&quot;},{&quot;_id&quot;:&quot;5f4ea4136290790025773399&quot;,&quot;term&quot;:&quot;Workflow&quot;,&quot;definition&quot;:&quot;The medium where VTEX and marketplaces interact about a specific event on an offer. Every event that happens to an offer is generated by adding a workflow.&quot;},{&quot;_id&quot;:&quot;5f4ea421bdcf6c002d55c46c&quot;,&quot;term&quot;:&quot;workflow&quot;,&quot;definition&quot;:&quot;The medium where VTEX and marketplaces interact about a specific event on an offer. Every event that happens to an offer is generated by adding a workflow.&quot;},{&quot;_id&quot;:&quot;5f4ea43e9ea4e5002ac3f267&quot;,&quot;term&quot;:&quot;error&quot;,&quot;definition&quot;:&quot;All problems related to the offers that require attention and analysis for manual re